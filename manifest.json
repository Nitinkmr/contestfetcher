{
"manifest_version": 2,
"name": "contestHunter",
"description": "This extension provides a list of all ongoing and upcoming programming contests",
"version": "1.0",
"browser_action": {
"default_icon": "img/logo.png",
"default_popup": "/index.html"
},
 "permissions": [
 	 "https://clist.by/",
 	 "http://clist.by/",
 	 "tabs" 
 ],
"content_security_policy": "script-src 'self' https://clist.by/api/v1/jsonp/contest/?username=nitin&api_key=b1361c1467e8bfe2d4a2b181aa95c1bfa1e9577d; object-src 'self'"

}
