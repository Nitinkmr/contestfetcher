{
"manifest_version": 2,
"name": "contestHunter",
"short_name":"contestHunter",
"description": "Never miss a Coding Contenst again! This extensions provids you with a list of all the ongoing and upcoming coding contests",
"version": "1.0",
"browser_action": {
"default_icon": "img/logo.png",
"default_popup": "/index.html"
},
 "icons": { "48": "img/logo.png" },
 "permissions": [
 	 "https://clist.by/",
 	 "http://clist.by/",
 	 "tabs" 
 ],
"content_security_policy": "script-src 'self' http://contesttrackerapi.herokuapp.com/; object-src 'self'"

}
